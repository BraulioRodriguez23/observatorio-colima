{
    "version": 2,
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/service-worker.js",
        "dest": "/service-worker.js",
        "headers": {
          "Cache-Control": "no-cache"
        }
      }
    ]
  }